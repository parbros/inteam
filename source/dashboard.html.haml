      Reporte Semanal
      \  
      %small Jun 15 - Jun 22, 2011
      \  
    .btn-group
      %button.btn.btn-default.btn-sm.dropdown-toggle{"data-toggle" => "dropdown", type: "button"}
        %i.fa.fa-clock-o
        \ 
        Seleccionar Semana
        %span.caret
      %ul.dropdown-menu{role: "menu"}
        %li
          %a{href: "javascript:;"} Action
        %li
          %a{href: "javascript:;"} Another action
        %li
          %a{href: "javascript:;"} Something else here
        %li.divider
        %li
          %a{href: "javascript:;"} Separated link
