    .col-md-3.col-sm-6
      %a.dashboard-stat.primary{href: "javascript:;"}
        .visual
          %i.fa.fa-star
        / /.visual
        .details
          %span.content Nuevos Tickets
          %span.value 1,236
        / /.details
        %i.fa.fa-play-circle.more
      / /.dashboard-stat
    / /.col-md-3
    .col-md-3.col-sm-6
      %a.dashboard-stat.secondary{href: "javascript:;"}
        .visual
          %i.fa.fa-shopping-cart
        / /.visual
        .details
          %span.content Tickets Terminados
          %span.value 256
        / /.details
        %i.fa.fa-play-circle.more
      / /.dashboard-stat
    / /.col-md-3
  
